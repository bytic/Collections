{
  "name": "bytic/collections",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/ByTIC/collections",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Collections\\": "src"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/functions/general.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Collections\\Tests\\Fixtures\\": "tests/fixtures",
      "Nip\\Collections\\Tests\\": "tests/src"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "bytic/utility": "^1.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0.14"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
